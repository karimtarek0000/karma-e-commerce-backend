{
  "rewrites": [{ "source": "/(.*)", "destination": "/api/server.js" }],
  "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
  "routes": [
      {
         "src": "/(.*)",
         "dest": "/api/server.js",
          "methods": ["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
             "Access-Control-Allow-Origin": ["https://karma-e-commerce-mla4az8ux-karimtarek0000s-projects.vercel.app", "https://karma-e-commerce-karimtarek0000s-projects.vercel.app"],
             "Access-Control-Allow-Credentials": "true",
             "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
         }
     }
   ]
}
