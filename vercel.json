{
  "rewrites": [{ "source": "/(.*)", "destination": "/api/server.js" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://karma-e-commerce-backend-karimtarek0000s-projects.vercel.app/"
      }
    }
  ]
}
